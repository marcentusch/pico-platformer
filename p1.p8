pico-8 cartridge // http://www.pico-8.com
version 32
__lua__

function _init()
  pl={
    sp=100,
    x=64,
    y=-20,
    w=8,
    h=8,
    flipx=false,
    dx=0,
    dy=0,
    max_dx=3,
    max_dy=3,
    acc_x=0.5,
    acc_y=4,
    anim=0,
    running=false,
    jumping=false,
    falling=false,
    sliding=false,
    landed=false,
  }

  gravity=0.3
  friction=0.8
end

function _update()

  update_player()
  animate_player()
end
 
function _draw()
  cls(12)
  map(1)
  spr(pl.sp,pl.x,pl.y,pl.w/8, pl.h/8, pl.flip)
end
-->8

-- collisions
function collide_map(obj,aim,flag)
  -- we create a box based on dirextion, then check if any sprite with a collision flag is touching
  local x=obj.x local y=obj.y local w=obj.w local h=obj.h
  local x1=0 local y1=0 local x2=0 local y2=0

  if aim=="left" then
    x1=x-1 y1=y 
    x2=x y2=y+h-1
  elseif aim=="right" then
    x1=x+w y1=y 
    x2=x+w+1 y2=y+h-1
  elseif aim=="up" then
    x1=x+1 y1=y-1 
    x2=x+w+1 y2=y+h-1
  elseif aim=="down" then
    x1=x y1=y+h 
    x2=x+w y2=y+h
  end

  -- pixels to tiles
  x1/=8 y1/=8  x2/=8 y2/=8

  -- check if any of the box corners is touching a sprite with collision flag
  if fget(mget(x1,y1), flag)
  or fget(mget(x1,y2), flag)
  or fget(mget(x2,y1), flag)
  or fget(mget(x2,y2), flag) then
    return true
  else
    return false
  end
end

-- Player
-- Buttons: 0..5: left right up down button_o button_x
function update_player() 
  -- apply physics
  pl.dy+=gravity
  pl.dx*=friction

  -- controls
  if btn(0) then
    pl.dx-=pl.acc_x
    pl.running=true
    pl.flip=true
  end
  if btn(1) then
    pl.dx+=pl.acc_x
    pl.running=true
    pl.flip=false
  end

  -- sliding
  if pl.running
  and not btn(0)
  and not btn(1)
  and not pl.falling
  and not pl.jumping then
    pl.running = false
    pl.sliding = true
  end

  -- jump
  if btnp(4)
  and pl.landed then
    pl.dy-=pl.acc_y
    pl.landed = false
  end

  -- check collision up and down
  if pl.dy>0 then
    pl.falling=true
    pl.landed=false
    pl.jumping=false

    if collide_map(pl,"down",0) then
      pl.landed=true
      pl.falling=false
      pl.dy=0
      -- make sure player is does not land under collision barrier
      pl.y-=(pl.y+pl.h)%8
    end    
  elseif pl.dy<0 then
    pl.jumping=true
    
    if collide_map(pl,"up", 1) then
      pl.dy=0 
    end
  end 

  -- checl left and right collision
  if pl.dx<0 then
    if collide_map(pl,"left",1) then
      pl.dx=0
    end
  elseif pl.dx>0 then
    if collide_map(pl,"right",1) then
      pl.dx=0
    end
  end

  -- stop sliding
  if pl.sliding then
    if abs(pl.dx)<.2
    or pl.running then
      pl.dx=0
      pl.sliding=false
    end
  end

  -- update player position
  pl.x+=pl.dx
  pl.y+=pl.dy
end

function animate_player()
  if pl.jumping then pl.sp=102
  elseif pl.falling then pl.sp=103
  elseif pl.running then
    if time()-pl.anim>.1 then
      pl.anim=time()
      pl.sp+=1
      if (pl.sp> 104) pl.sp=100
    end
  else -- idle
    pl.sp=100
  end
end

